Program Hello; // ubah menjadi Program APCI-NIM
            Uses Crt;
            Begin
                ClrScr;
                Writeln(‘Hello’);//untuk mengeluarkan string ‘Hello’ di layar
                Writeln(‘1301153654’); //Ubah untuk menampilkan NIM Anda
                Writeln(‘Rifa’); //Ubah untuk menampilkan Nama Anda
                Writeln(‘IF39INT’); //Ubah untuk menampilkan Kelas Anda
                Writeln(‘Wida’); //Ubah untuk menampilkan Nama Asprak Anda
                Writeln(‘1301153630’); //Ubah untuk menampilkan NIM teman anda
                Writeln(‘Modul 6’); //Ubah untuk menampilkan string ‘Modul 6
            End.
